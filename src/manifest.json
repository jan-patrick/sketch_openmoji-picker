      "shortcut": "ctrl o",
      "handler": "openWindowToAddOpenMoji",
      "icon": "iconRunner.png",
      "iconDark": "iconRunnerDark.png",
      "description": "Lets you insert an OpenMoji via the official website."
    },
    {
      "script": "my-command.js",
      "handlers": {
        "actions": {
          "Startup": "onStartup",
          "Shutdown": "onShutdown",
          "SupplyData": "onSupplyData"
        }
      }
    }
  ],
  "menu": {
    "isRoot": true,
    "items": [
      "OpenMojiPicker_openWindowToAddOpenMoji"
    ]
  }
