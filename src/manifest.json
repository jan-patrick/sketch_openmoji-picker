{
  "name": "OpenMoji Picker",
  "description": "Add OpenMoji to your Sketch project",
  "author": "Jan Schneider",
  "authorEmail": "hello@jan-patrick.de",
  "homepage": "https://github.com/jan-patrick/sketch_openmoji-picker",
  "compatibleVersion": 54.1,
  "bundleVersion": 1,
  "version": "0.1.3",
  "identifier": "com.github.jan-patrick.sketch_openmoji-picker",
