{
  "name": "openmoji-picker",
  "version": "0.1.4-0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "OpenMoji Picker",
    "manifest": "src/manifest.json",
    "main": "openmoji-picker.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11",
    "@skpm/extract-loader": "^2.0.2",
    "css-loader": "^1.0.0",
    "html-loader": "^0.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jan-patrick/sketch_openmoji-picker.git"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "sketch-module-web-view": "^3.1.0"
  },
  "author": "jan-patrick <jan.schneider@hfg-gmuend.de>"
}
